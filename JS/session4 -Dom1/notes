-- DOM ==> Document Object Model

-- document ==> .getElementById(elementID) ==> return 1 element
                .getElementsByClassName(elementClass) ==> return Array
                .getElementsByTagName(tag) ==> return Array
                .querySelector(css selector) ==> return 1 element
                .querySelectorAll(css selector) ==> return Array


